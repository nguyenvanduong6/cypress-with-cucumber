{
  "name": "cypress-cb",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "cypress:open-dev": "cross-env E2E_VARIANT=.dev cypress open",
    "cypress:open-ci": "cross-env E2E_VARIANT=.ci cypress open",
    "cypress:open-stg": "cross-env E2E_VARIANT=.stg cypress open",
    "cypress:open-prod": "cross-env E2E_VARIANT=.prod cypress open",
    "cypress:run-dev": "cross-env E2E_VARIANT=.dev cypress run --browser chrome; npm run generate-report",
    "cypress:run-ci": "cross-env E2E_VARIANT=.ci cypress run",
    "cypress:run-stg": "cross-env E2E_VARIANT=.stg cypress run --browser chrome; npm run generate-report",
    "cypress:run-prod": "cross-env E2E_VARIANT=.prod cypress run --browser chrome; npm run generate-report",
    "clean:reports": "rm -R -f cypress/reports && rm -R -f ./report && mkdir cypress/reports",
    "pretest": "npm run clean:reports",
    "generate-report": "node generate-cucumber-html-reporter.ts",
    "cy:run": "cross-env E2E_VARIANT=.ci cypress run --browser chrome ; npm run generate-report",
    "cypress:info": "cypress info",
    "lint": "eslint --max-warnings 0 --ext '.ts' 'cypress/**/*.ts'",
    "format": "prettier --config ./.prettierrc.yml --write 'cypress/**/*.ts'",
    "format:check": "prettier --config ./.prettierrc.yml --check 'cypress/**/*.ts'",
    "danger-ci": "danger ci",
    "danger-local": "danger pr",
    "find-file-changed": "cross-env DEBUG=find-cypress-specs:git find-cypress-specs --branch $BASE_BRANCH --trace-imports cypress --set-gha-outputs"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@badeball/cypress-cucumber-preprocessor": "^22.0.1",
    "@bahmutov/cypress-esbuild-preprocessor": "^2.2.4",
    "@seadub/danger-plugin-eslint": "^3.0.2",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "cross-env": "^7.0.3",
    "cypress": "^13.17.0",
    "cypress-file-upload": "^5.0.8",
    "cypress-recurse": "^1.35.3",
    "danger": "^11.2.6",
    "dotenv": "^16.0.3",
    "esbuild": "^0.25.2",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-cypress": "^2.13.2",
    "find-cypress-specs": "^1.33.2",
    "multiple-cucumber-html-reporter": "^3.4.0",
    "prettier": "^2.8.7",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "fast-xml-parser": "^5.2.3",
    "node-fetch": "^3.3.2",
    "node-mailjet": "^6.0.8"
  },
  "cypress-cucumber-preprocessor": {
    "stepDefinitions": [
      "cypress/e2e/step_definitions/[filepath].{js,ts}",
      "cypress/e2e/step_definitions/common.{js,ts}"
    ],
    "json": {
      "enabled": true,
      "formatter": "./cucumber-json-formatter",
      "output": "cypress/reports/cucumber-json-report.json"
    },
    "html": {
      "enabled": false
    }
  }
}
